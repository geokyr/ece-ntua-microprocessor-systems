START:
	IN 10H
	CALL MAIN
	RST 1

MAIN:
	MVI A,00H
	LXI H,0900H
	LXI B,0000H
	MVI D,00H

FIRST:
	CALL SECOND
	CPI FFH
	JZ ENDED

	CPI 10H
	JC OUTSIDE
	CPI 61H
	JC INSIDE
	JMP OUTSIDE

INSIDE:
	INR D

OUTSIDE:
	MOV M,A
	INX H
	INR A
	JMP FIRST

ENDED:
	MOV M,A
	RET

SECOND:
	RAR
	JNC COUNT1
	INX B

COUNT1:
	RAR
	JNC COUNT2
	INX B

COUNT2:
	RAR
	JNC COUNT3
	INX B

COUNT3:
	RAR
	JNC COUNT4
	INX B

COUNT4:
	RAR
	JNC COUNT5
	INX B

COUNT5:
	RAR
	JNC COUNT6
	INX B

COUNT6:
	RAR
	JNC COUNT7
	INX B

COUNT7:
	RAR
	JNC COUNT8
	INX B

COUNT8:
	RAR
	RET

END